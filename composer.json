{
    "name": "georgie/laravel-module",
    "description": "laravel modules",
    "license": "MIT",
    "authors": [
        {
            "name": "georgie",
            "email": "227040015@qq.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "\\Houdunwang\\Module\\LaravelServiceProvider"
            ],
            "aliases": {
                "HDModule": "\\Houdunwang\\Module\\Factory"
            }
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "nwidart/laravel-modules": "^3.3",
        "spatie/laravel-permission": "^2.12",
        "doctrine/dbal": "^2.7",
        "georgie/laravel-autocreate": "dev-main",
    },
    "autoload": {
        "psr-4": {
            "Houdunwang\\Module\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "houdunwang\\Module\\Tests\\": "tests/"
        }
    }
}
